    "@dcloudio/uni-mp-baidu": "^3.0.0-3080420230531001",
    "@dcloudio/uni-mp-jd": "^3.0.0-3080420230531001",
    "@dcloudio/uni-mp-kuaishou": "^3.0.0-3080420230531001",
    "@dcloudio/uni-mp-lark": "^3.0.0-3080420230531001",
