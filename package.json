    "@uni-helper/volar-service-uni-pages": "^0.2.1",
    "@vingogo/eslint-config-vue": "^0.2.2",
    "@vingogo/prettier-config": "^0.1.1",
    "@vingogo/stylelint-config": "^0.1.2",
    "@vingogo/vin-cli": "^0.6.0",
